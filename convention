Class ClassName {
// VARIABLE
 public:
	varType varName;
 private:
	varType	_varName; // private 멤버 변수나 함수는 앞에 _를 사용한다

// FUNCTION
 public:
	returnType funcName();
 private: // 접근 지정자 앞은 space 1칸
	returnType _funcName(); // type과 name 사이는  tab

// OCCF
 public:
 private:

// OPERATOR OVERRIDE
};

